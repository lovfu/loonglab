<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class dynamic-update="true" table="l_ip_location" name="com.kejikeji.lbs.model.IpLocation">
    <id name="id">
      <generator class="native"/>
    </id>
    <property name="ip"/>
    <many-to-one name="location" class="cn.kejikeji.lbs.model.LocationCatalog"/>
  </class>
</hibernate-mapping>
